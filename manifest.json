{
  "name": "Font Manager",
  "version": "0.1.0",
  "description": "My Chrome extension for managing fonts",
  "permissions": [
    "storage",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content_script.js"
      ],
      "css": [
        "styles.css"
      ]
    }
  ],
  "icons": {
    "16": "/assets/icons/extension-icons/icon16.png",
    "32": "/assets/icons/extension-icons/icon32.png",
    "64": "/assets/icons/extension-icons/icon64.png",
    "128": "/assets/icons/extension-icons/icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "/assets/icons/extension-icons/icon16.png",
      "32": "/assets/icons/extension-icons/icon32.png",
      "64": "/assets/icons/extension-icons/icon64.png",
      "128": "/assets/icons/extension-icons/icon128.png"
    },
    "default_title": "My Fonts",
    "default_popup": "popup.html"
  },
  "manifest_version": 3
}